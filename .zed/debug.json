[
  {
    "label": "Build & Debug native xqsqld",
    "build": {
      "command": "cargo",
      "args": ["build"]
    },
    "program": "$ZED_WORKTREE_ROOT/target/debug/qxsqld",
    "args": ["--config", "/home/fanda/t/1.yaml"],
    "env": { "RUST_LOG": "info" },
    // sourceLanguages is required for CodeLLDB (not GDB) when using Rust
    "sourceLanguages": ["rust"],
    "request": "launch",
    "adapter": "CodeLLDB"
  }
]
